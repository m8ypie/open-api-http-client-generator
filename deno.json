{
  "name": "@m8ypie/open-api-http-client-generator",
  "version": "0.0.2-beta",
  "description": "A Deno module to generate HTTP clients from OpenAPI specifications.",
  "exports": "./src/mod.ts",
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/path": "jsr:@std/path@^1.1.1",
    "@yellicode/core": "npm:@yellicode/core@^1.5.0",
    "@yellicode/templating": "npm:@yellicode/templating@^1.9.0",
    "@yellicode/typescript": "npm:@yellicode/typescript@^1.14.2",
    "cross-dirname": "npm:cross-dirname@^0.1.0",
    "openapi-typescript": "npm:openapi-typescript@^7.8.0",
     "ts-morph": "jsr:@ts-morph/ts-morph@^26.0.0",
     "ky": "npm:ky@^1.8.1",
     "open-api-http-client-generator": "./src/mod.ts",
     "x/case": "npm:change-case@^5.4.4"
  }
}
